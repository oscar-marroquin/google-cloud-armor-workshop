# Environment tag for all resources being created. You can leave this value as-is.
environment_tag =   "sbx" # sbx = Sandbox, dev = Development, uat = UAT, prd = Production
company_name    =   "ACME"
business_unit   =   "Cloud-Services" # Don't put spaces, instead use - or _.
project         =   "your-google-cloud-project-id"

# Variables to customize some rules inside the policy
admin_path         =   "/webapp/adminv2/" # If your web apps have some administration path, you can put this that path here to protect allowing only to a whitelist to access them.
ip_blacklist       =   "192.0.0.0/24" # If you known some bad IP addresses or IP ranges that are malicious you can put here this information separated by commas.
